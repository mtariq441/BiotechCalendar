{
  "version": 2,
  "builds": [
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/client/$1"
    },
    {
      "src": "/.*",
      "dest": "/dist/client/index.html"
    }
  ]
}
